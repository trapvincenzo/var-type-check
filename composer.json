{
    "name": "trapvincenzo/var-type-check",
    "description": "Simple var type checker for Twig",
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "twig/twig": "^1.35",
        "symfony/symfony": "^3.3"
    },
    "keywords": [
        "bundle",
        "cache",
        "symfony",
        "twig",
        "type"
    ],
    "require": {
        "php": "^5.6|^7.0"
    },
    "autoload": {
        "exclude-from-classmap": [
            "/Tests/"
        ],
        "psr-4": {
            "Trapvincenzo\\Bundle\\VarTypeCheckBundle\\": ""
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    },
    "authors": [
        {
            "name": "Vincenzo Trapani",
            "email": "trapvincenzo@gmail.com"
        }
    ]
}
